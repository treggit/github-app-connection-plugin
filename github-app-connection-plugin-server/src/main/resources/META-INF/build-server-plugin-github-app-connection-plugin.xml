<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">

    <bean class="jetbrains.buildServer.teamcity.github.api.GithubApiFacadeImpl"/>
    <bean class="jetbrains.buildServer.teamcity.github.connection.ApplicationConnectionTab"/>
    <bean class="jetbrains.buildServer.teamcity.github.connection.GithubApplicationConnectionManager"/>
    <bean class="jetbrains.buildServer.teamcity.github.connection.GithubApplicationConnectionController"/>
    <bean class="jetbrains.buildServer.teamcity.github.connection.GithubTokenParameterProvider"/>
    <bean class="jetbrains.buildServer.teamcity.github.connection.GithubTokensRegistry"/>
</beans>